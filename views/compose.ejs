<%-include("partials/header");-%>
<h1>Compose a Blog </h1>
<form class="form"  action="/compose" method="post">
    <div class="form-elements">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle">
        <label>PostContent</label>
        <textarea class="form-control" name="postContent" rows="5" cols="20"></textarea>

    </div>
    <button id="btn-primary" type="submit" name="button">Publish</button>

</form>
<%-include("partials/footer")  -%>